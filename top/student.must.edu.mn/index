                        //window.location = '/Stipend/SurveyVaccined';


                    }
                }
            });
        }

        function CheckValidPassword() {

            $.ajax({
                url: "/Home/CheckValidPassword",
                dataType: "json",
                type: "get",
                error: function () {
                    alert("Алдаа гарлаа");
                },
                success: function (data) {
                    if (!data) {
                        window.location = '/Home/ChangePassAction';
                    }
                }
            });
        }




        function CheckEmail() {
            $.ajax({
                url: "/Home/IsCheckEmail",
                dataType: "json",
                type: "get",
                error: function () {
                    alert("Алдаа гарлаа");
                },
                success: function (data) {
                    if (!data) {
                        window.location = '/Home/EmailChangeAction';
                    }
                }
            });
        }



        $('#exampleModal').modal({
            backdrop: 'static',
            keyboard: false,
            width: 'auto'
        })

        //surveyCheckGoogleForm();

        function surveyCheckGoogleForm() {
            $.ajax({
                //url: "/Student/Edit2021",
                //url: "/Home/IsCreateSurveyGoogleForm",
                dataType: "json",
            type: "get",
            error: function () {
                alert("Алдаа гарлаа1");
            },
            success: function (data) {
                if (!data) {
                    $('#exampleModal').modal('show');
                } else {
                    $('#exampleModal').modal('hide');
                }
            }
        });
        }

        $('#exampleModal').on('hidden.bs.modal', function () {
            $.ajax({
                //url: "/Student/Edit2021",
                //url: "/Home/CreateSurveyGoogleForm",
                dataType: "json",
            type: "get",
            error: function () {
                alert("Алдаа гарлаа1");
            },
            success: function (data) {
                if (!data) {
                    location.reload();
                }
            }
        });
        })
    </script>
    
