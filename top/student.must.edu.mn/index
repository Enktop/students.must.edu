        //})

        if (window.localStorage.getItem("openable") != null) {
            var activeindex = window.localStorage.getItem("openable");
            $('.openable:eq(' + (activeindex - 1) + ')').addClass("active");
        }

        $('.menu .openable').click(function () {
            var self = $(this),
                index = self.index();
            window.localStorage.setItem("openable", index);
        });

