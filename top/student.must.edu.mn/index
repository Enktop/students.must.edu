<script type="text/javascript">

    var series = new Array();
                var xAsis = new Array();
                $.ajax({
                    url: "/Home/GpaChart",
                    dataType: "json",
                    type: "GET",
                    contentType: 'application/json; charset=utf-8',
                    async: false,
                    processData: false,
                    cache: false,
                    delay: 15,
                    success: function (data) {
                        for (var i in data) {
                            var serie = new Array(data[i].Value, data[i].Item);
                            series.push(data[i].Item);
                            xAsis.push(data[i].Value);
                        }
                    },
                    error: function (xhr) {
                        alert('error');
                    }
                });

    $(function () {
        Highcharts.setOptions({
            chart: {
                style: {
                    fontFamily: 'Arial, Helvetica, sans-serif',
                    
                },
                //backgroundColor: 'rgba(255, 128, 128, 0.1)',
            }
        });

        $('#container').highcharts({
            title: {
                style: {
                    fontFamily: 'Arial, Helvetica, sans-serif',
                    fontSize: 12
                },
                text: '',
                x: -20, //center
            },

            xAxis: {
                categories: xAsis,
                title: {
                    text: 'Хичээлийн жил, улирал'
                },
            },
            yAxis: {
                tickPositions: [0, 0.5, 1, 1.5, 2, 2.5, 3, 3.5, 4],
                title: {
                    text: 'Чанарын оноо'
                },
                plotLines: [{
                    value: 0,
                    width: 1
                }]
            },
            legend: {
                layout: 'vertical',
                align: 'right',
                verticalAlign: 'middle',
                borderWidth: 0
            },
            series: [{
                name: 'ҮГД',
                data: series,
                zones: [{
                    value: 2.0,
                    color: '#E38585'
                }, {
                    value: 2.8,
                    color: '#7cb5ec'
                }, {
                    color: '#55EE35'
                }]
            }]
        });
    });

</script>








